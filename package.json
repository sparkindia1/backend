{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "ci": "yarn install --production",
    "build": "tsc",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "*",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "16.18.0"
  },
  "packageManager": "yarn@1.22.19",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
